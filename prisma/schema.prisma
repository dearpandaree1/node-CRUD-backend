generator client {
  provider = "prisma-client-js"
}


datasource db {
  url      = env("DATABASE_URL")
  provider = "mysql"
}


model Address {
  id            Int      @id @default(autoincrement())
  firstName     String
  lastName      String   
  phoneNumber   String?  
  address1      String?
  address2      String?
  city          String?
  zipCode       Int
}